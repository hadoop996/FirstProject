<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.TestMapper">

	<select id="getSql1" resultType="String">
	  select username from ${table}
	</select>

	<insert id="insertSql" parameterType="java.util.List">
		insert into test_insert(name ,age)
		values
		<foreach collection="list" item='pa' index="index" separator=','>
			(#{pa.name}, #{pa.age})
		</foreach>
	</insert>

	<insert id="insertUser" >
		insert ignore into t_user_engineer_rel(user_broadcast ,user_phone)
		values ('123123','123123')
	</insert>

	<insert id="insertEngineerUserList" parameterType="java.util.List">
		insert into t_henan(engineer_phone,brandbond_code) values
		<foreach collection="param1" item='pa' index="index" separator=','>
			${pa}
		</foreach>

	</insert>

    <insert id="insertUserList">
		insert into t_beijing_03194(engineer_phone,brandbond_code) values
		<foreach collection="userEngineerPOS" item='pa' index="index" separator=','>
			(#{pa.engineerPhone},#{pa.brandbondCode})
		</foreach>

	</insert>
</mapper>
